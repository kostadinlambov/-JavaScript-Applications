<section id="{{sectionId}}" class="{{sectionId}}">
    <h1 class="titleForm">{{titleText}}</h1>
    <form id="{{formId}}" class="form">
        <div>Username:</div>
        <div><input type="text" name="username" required/></div>
        <div>Password:</div>
        <div><input type="password" name="passwd" required/></div>
        <div><input type="button" id="{{buttonId}}" value="{{buttonValue}}"/></div>
    </form>
</section>

{{> footer}}
